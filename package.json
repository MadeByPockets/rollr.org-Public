{
  "name": "@pockets/shared-ui",
  "version": "0.1.11",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./components/*": {
      "types": "./dist/shared-ui/components/*.d.ts",
      "import": "./dist/shared-ui/components/*.mjs",
      "require": "./dist/shared-ui/components/*.cjs"
    },
    "./components/EventPage": {
      "types": "./dist/shared-ui/components/EventPage.d.ts",
      "import": "./dist/shared-ui/components/EventPage.mjs",
      "require": "./dist/shared-ui/components/EventPage.cjs"
    },
    "./components/common": {
      "types": "./dist/shared-ui/components/common.d.ts",
      "import": "./dist/shared-ui/components/common.mjs",
      "require": "./dist/shared-ui/components/common.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "_release": "node scripts/release.mjs",
    "publish:web": "npm run _release",
    "publish:web:canary": "node scripts/release.mjs --channel canary",
    "publish:web:nightly": "node scripts/release.mjs --channel nightly",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test:publish": "node scripts/test-publish.mjs",
    "update:barrels": "node scripts/update-barrels.mjs"
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/icons-material": "^7",
    "@mui/joy": "^5.0.0-beta",
    "@mui/material": "^7",
    "next": "^15",
    "react": "^19",
    "react-dom": "^19"
  },
  "peerDependenciesMeta": {
    "@mui/icons-material": {
      "optional": true
    },
    "@mui/joy": {
      "optional": true
    }
  },
  "devDependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/joy": "^5.0.0-beta.52",
    "@mui/material": "^7.3.4",
    "@types/node": "^20",
    "next": "^15.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
